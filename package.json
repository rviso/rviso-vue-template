{
  "name": "blanc-nova-vue3-template",
  "type": "module",
  "version": "0.0.1",
  "description": "A Vue 3 template for Blanc Nova",
  "author": "lingann",
  "license": "ISC",
  "homepage": "https://github.com/Lingann/blanc-nova-vue3-template",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lingann/blanc-nova-vue3-template.git"
  },
  "bugs": {
    "url": "https://github.com/Lingann/blanc-nova-vue3-template/issues"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "LICENSE",
    "README.md",
    "dist",
    "es",
    "lib"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "prepare": "husky install && node ./scripts/prepare.mjs",
    "postinstall": "patch-package",
    "lint:eslint": "eslint --ext \".js .jsx .ts .tsx .vue\" src",
    "lint:prettier": "prettier --check .",
    "lint:stylelint": "stylelint src/**/*.{css,less,scss,vue,sass}",
    "lint": "npm run lint:eslint && npm run lint:prettier && npm run lint:stylelint",
    "lint:fix": "prettier --write --list-different . && npm run lint:eslint --fix && npm run lint:stylelint --fix",
    "dev": "vite",
    "build": "vite build",
    "test": "vitest -u",
    "coverage": "vitest run --coverage"
  },
  "browserslist": [
    "> 5%",
    "last 2 versions",
    "not ie <= 11"
  ],
  "dependencies": {
    "pinia": "^2.1.7",
    "vue": "3.3.10",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@rviso/eslint-config-standard": "^0.1.0",
    "@rviso/eslint-config-typescript": "^0.1.0",
    "@rviso/eslint-config-vue": "^0.1.0",
    "@types/mockjs": "^1.0.10",
    "@types/node": "^18.19.33",
    "@types/three": "^0.164.0",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "mockjs": "^1.1.0",
    "patch-package": "^6.5.1",
    "postcss": "^8.4.38",
    "postcss-html": "^1.7.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^2.8.8",
    "sass": "1.56.2",
    "stylelint": "^15.11.0",
    "stylelint-config-recess-order": "^4.6.0",
    "stylelint-config-recommended": "^13.0.0",
    "stylelint-config-standard": "^31.0.0",
    "stylelint-config-standard-scss": "^11.1.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^3.0.0",
    "tailwindcss": "^3.4.3",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^4.9.5",
    "vite": "^4.5.3",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-mock": "^3.0.2",
    "vitest": "^1.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue,html}": [
      "eslint",
      "stylelint"
    ],
    "*.{scss,less,style}": [
      "stylelint"
    ]
  }
}
